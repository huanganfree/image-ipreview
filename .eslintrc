{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "vue-eslint-parser", // 解析器
  "extends": "elemefe", // 使用element配置规则
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module", 
    "ecmaFeatures": {
      "jsx": true  
    }
  },
  "rules": {
    "no-debugger": "error"
  }
}
